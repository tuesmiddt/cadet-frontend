    "@types/common-tags": "^1.4.0",
    "@types/dotenv": "^6.1.1",
    "@types/draft-js": "^0.10.23",
    "@types/enzyme": "3.1.17",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/estree": "^0.0.39",
    "@types/invariant": "^2.2.29",
